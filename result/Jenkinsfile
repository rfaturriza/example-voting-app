pipeline {  
    agent any  
    tools{    
        nodejs 'nodeJS 8.9.0'  
    }  
    stages{      
        stage("build"){  
            steps{              
                echo 'Compiling result app'              
                dir('result'){                
                    sh 'npm install'              
                }          
            }      
        }      
        stage("test"){   
            steps{              
                echo 'Running Unit Tets on result app'  
                dir('worker'){                
                    sh 'npm install'            
                    sh 'npm test'              
                }          
            }      
        }   
    }  
    post{    
        always{  
            echo 'Building multibranch pipeline for worker is completed..'
        }  
    }
}